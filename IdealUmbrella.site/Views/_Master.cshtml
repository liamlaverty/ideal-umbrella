@using System.ComponentModel;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
}
<!DOCTYPE html>
<html lang="@Model.GetCultureFromDomains()" class="h-100">
	<head>
		    <meta charset="utf-8">
			    <meta name="viewport" content="width=device-width, initial-scale=1">
		<title>
			@{
				if (Model.HasValue("sEOTitle"))
				{
					@("seo title ")
					@Model.Value("sEOTitle");
				}
				else 
				{
					@Model.Value("pageTitle");
				}
			}
		</title>
		<meta name="description" content="@Model.Value("sEODescription")">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
	<script src='https://api.mapbox.com/mapbox-gl-js/v2.9.1/mapbox-gl.js'></script>
	<link href='https://api.mapbox.com/mapbox-gl-js/v2.9.1/mapbox-gl.css' rel='stylesheet' />
	</head>

<body id="body" class="d-flex flex-column h-100" data-mbkey="@(await Component.InvokeAsync("MapboxScript"))">
	<main class="flex-shrink-0">
		@Html.Partial("/Views/Partials/Widgets/MainMenu.cshtml")

		<div class="container">
		@Html.Partial("Partials/Widgets/SiteStructure/Header.cshtml")

			@RenderBody()
	</main>
			@Html.Partial("Partials/Widgets/SiteStructure/Footer.cshtml")
		</div>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
		<script src="https://cdn.jsdelivr.net/npm/@@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.min.js" integrity="sha384-Rx+T1VzGupg4BHQYs2gCW9It+akI2MM/mndMCy36UVfodzcJcF0GGLxZIzObiEfa" crossorigin="anonymous"></script>

	<link rel="stylesheet" href="/static/bs-icons/font/bootstrap-icons.css">
		@*scripts/mapbox.js must be at the end of the document*@
		<script src="/scripts/mapbox.js"></script>
		<link href='/styles/site.css' rel='stylesheet' />

	
	</body>



</html>